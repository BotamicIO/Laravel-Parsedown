{
    "name": "faustbrian/laravel-parsedown",
    "description": "Parsedown Wrapper for Laravel 5",
    "keywords": ["laravel", "framework", "Laravel-Parsedown", "Laravel Parsedown", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de"
    }],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.3.* || 5.4.* || 5.5.*",
        "erusev/parsedown": "^1.6"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Parsedown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Parsedown\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\Parsedown\\ParsedownServiceProvider"
            ],
            "aliases": {
                "Parsedown": "BrianFaust\\Parsedown\\Facades\\Parsedown"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
