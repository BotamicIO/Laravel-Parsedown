{
    "name": "draperstudio/laravel-parsedown",
    "type": "library",
    "description": "Parsedown Wrapper for Laravel 5",
    "keywords": [
        "draperstudio",
        "laravel-parsedown"
    ],
    "homepage": "https://github.com/DraperStudio/laravel-parsedown",
    "license": "MIT",
    "authors": [{
        "name": "DraperStudio",
        "email": "hello@draperstudio.tech",
        "homepage": "http://draperstudio.tech",
        "role": "Developer"
    }],
    "require": {
        "php": "~5.5|~7.0",
        "illuminate/support": "5.1.*|5.2.*",
        "erusev/parsedown": "^1.6"
    },
    "require-dev": {
        "orchestra/testbench": "3.1.*|3.2.*",
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "DraperStudio\\Parsedown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DraperStudio\\Parsedown\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
